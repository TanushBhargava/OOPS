class Geeks {
    public static void main(String[] args) {
        byte[] byteArray1 = { 84, 65, 78, 85, 83, 72 };
        String str1 = new String(byteArray1);
        System.out.println("Name is: " + str1);

        byte[] byteArray2 = { 116, 97, 110, 117, 115, 104, 46, 98, 104, 97, 114, 103, 97, 118, 46, 99, 115, 50, 56, 64, 105, 105, 108, 109, 46, 101, 100, 117 };
        String str2 = new String(byteArray2);
        System.out.println("Email is: " + str2);
    }
}
